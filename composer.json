{
    "name": "cakemanager/cakephp-whosonline",
    "description": "WhosOnline plugin for CakePHP to check your users activity",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": " 4.6.*@dev"
    },
    "autoload": {
        "psr-4": {
            "WhosOnline\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WhosOnline\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "App\\": "tests/App"
        }
    },
    "minimum-stability" : "dev"
}
