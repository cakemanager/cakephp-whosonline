{
    "name": "cakemanager/cakephp-whosonline",
    "description": "WhosOnline plugin for CakePHP to check your users activity",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "WhosOnline\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WhosOnline\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
